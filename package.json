{
  "name": "n8n-nodes-kargo-entegrator",
  "version": "0.1.0",
  "description": "N8N node for Kargo Entegratör API integration - create shipments and returns",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://github.com/kargo-entegrator/n8n-nodes-kargo-entegrator#readme",
  "author": {
    "name": "Kargo Entegratör",
    "email": "destek@kargoentegrator.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kargo-entegrator/n8n-nodes-kargo-entegrator.git"
  },
  "bugs": {
    "url": "https://github.com/kargo-entegrator/n8n-nodes-kargo-entegrator/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/kargo-entegrator"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "dev:live": "concurrently \"npm run dev\" \"npm run dev:docker:restart\"",
    "dev:docker:restart": "docker-compose -f docker-compose.simple.yml restart",
    "dev:docker": "docker-compose -f docker-compose.dev.yml up -d",
    "dev:docker:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "dev:docker:stop": "docker-compose -f docker-compose.dev.yml down",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/KargoEntegratorApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/KargoEntegrator/KargoEntegrator.node.js",
      "dist/nodes/KargoEntegrator/KargoEntegratorTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "~8.32.0",
    "concurrently": "^9.2.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}
